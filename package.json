    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "prettier": "^3.7.4",
    "terser": "^5.44.1",
