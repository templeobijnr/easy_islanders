  "dependencies": {
    "@google/genai": "^1.30.0",
    "@supabase/supabase-js": "2.39.0",
    "@types/mapbox-gl": "^3.4.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.554.0",
    "mapbox-gl": "^3.16.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.3",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
