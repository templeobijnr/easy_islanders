    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky",
    "guard:services": "npx tsx scripts/guard-services-structure.ts",
    "audit:frontend": "npx tsx scripts/audit-frontend.ts"
